<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/test"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imageButtonReset"
        android:layout_width="65dp"
        android:layout_height="19dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="248dp"
        android:background="@drawable/transparent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewReset"
        android:layout_width="63dp"
        android:layout_height="19dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="248dp"
        android:fontFamily="@font/bauhausregular"
        android:gravity="center"
        android:text="Reset"
        android:textColor="#B2FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonStop"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="336dp"
        android:layout_marginTop="192dp"
        android:background="@drawable/playback2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="63dp"
        android:layout_height="37dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="232dp"
        android:fontFamily="@font/bauhausregular"
        android:gravity="center"
        android:text="Playback/Stop"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonDrums"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/mediadrum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="92dp"
        android:layout_height="28dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/bauhausregular"
        android:gravity="center"
        android:text="Drums"
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonDrums" />

    <ImageButton
        android:id="@+id/imageButtonGuitar"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/mediaguitar"
        app:layout_constraintEnd_toStartOf="@+id/imageButtonPiano"
        app:layout_constraintStart_toEndOf="@+id/imageButtonDrums"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="92dp"
        android:layout_height="28dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="28dp"
        android:fontFamily="@font/bauhausregular"
        android:gravity="center"
        android:text="Guitar"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonGuitar" />

    <ImageButton
        android:id="@+id/imageButtonPiano"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/mediapiano"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="92dp"
        android:layout_height="28dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="40dp"
        android:fontFamily="@font/bauhausregular"
        android:gravity="center"
        android:text="Piano"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonPiano" />

    <Button
        android:id="@+id/kickid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginBottom="224dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige"
        android:fontFamily="@font/bauhausregular"

        android:text="Kick"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/snareid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="164dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige3"
        android:fontFamily="@font/bauhausregular"

        android:text="Snare"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/bellid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginBottom="100dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige2"
        android:fontFamily="@font/bauhausregular"

        android:text="Bell"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/hihatclosedid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="164dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige4"
        android:fontFamily="@font/bauhausregular"

        android:text="Closed-Hat"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/sidestickid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="288dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige6"
        android:fontFamily="@font/bauhausregular"

        android:text="Side-Stick"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/crashid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginBottom="348dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige7"
        android:fontFamily="@font/bauhausregular"

        android:text="Crash"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/hihatopenid"
        android:layout_width="125dp"
        android:layout_height="110dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="288dp"
        android:background="@drawable/hexagon"
        android:backgroundTint="@color/beige5"
        android:fontFamily="@font/bauhausregular"

        android:text="Open-Hat"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/bauhausregular"
        android:text="Acoustic"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Switch
        android:id="@+id/switch1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="4dp"
        android:checked="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView5"
        app:layout_constraintStart_toEndOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/bauhausregular"
        android:text="Electronic"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <SeekBar
        android:id="@+id/seekBar"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarKick"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarSnare"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarHiHatOpen"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarHiHatClosed"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarStick"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarBell"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarCrash"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarReverb"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBarDelay"
        android:layout_width="120dp"
        android:layout_height="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="204dp"
        android:max="10"
        android:progress="10"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButtonSequencer"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="264dp"
        android:layout_marginTop="192dp"
        android:background="@drawable/record"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/theRedCircle"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="274dp"
        android:layout_marginTop="202dp"
        android:background="@drawable/redcircleforsequencer"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="252dp"
        android:layout_marginTop="232dp"
        android:fontFamily="@font/bauhausregular"
        android:gravity="center"
        android:text="Sequencer"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/drumReverb"
        android:layout_width="65dp"
        android:layout_height="85dp"
        android:layout_marginStart="17dp"
        android:background="@drawable/drumreverb"
        android:fontFamily="@font/bauhausregular"
        android:gravity="bottom"
        android:text=" Reverb"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/drumDelay"
        android:layout_width="65dp"
        android:layout_height="85dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/drumdelay"
        android:fontFamily="@font/bauhausregular"
        android:gravity="bottom"
        android:text="  Delay"
        android:textColor="@color/white"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/buttonVolume"
        android:layout_width="122dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="224dp"
        android:color="@color/white"
        android:fontFamily="@font/bauhausregular"
        android:text="Volume Control"
        android:textSize="10dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>
